{
	"name": "phpstan/phpstan-doctrine",
	"description": "Doctrine extensions for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.8-dev"
		}
	},
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "^0.8"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.13.0",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.16.0",
		"phpunit/phpunit": "^5.7",
		"slevomat/coding-standard": "^2.0",
		"doctrine/common": "^2.7",		
 		"doctrine/orm": "^2.5"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
